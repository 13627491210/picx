.feedback-page-container {

  .help-info-item {
    display flex
    margin-bottom 10rem
    padding 6rem
    font-size 16rem

    &:last-child {
      margin-bottom 0
    }
  }

  .description {
    font-weight bold
    line-height 28rem
  }

  .red-text {
    color var(--el-color-danger)
    font-size 20rem
  }
}
